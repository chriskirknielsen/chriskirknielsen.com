{
	"defaultTheme": {
		"dark": "outrun",
		"light": "vapor"
	},
	"defaultVars": {
		"ckn-body-bg": "white",
		"ckn-body-text": "black",
		"ckn-body-link": "blue",
		"heading-tracking": "0",
		"corner-radius": "8px",
		"border-style": "solid"
	},
	"themes": {
		"outrun": {
			"label": "Outrun",
			"colors": {
				"grey": {
					"min": "hsl(225, 15%, 95%)",
					"med": "hsl(227, 5%, 63%)",
					"max": "hsl(233, 12%, 13%)"
				},
				"primary": {
					"min": "hsl(33, 100%, 88%)",
					"med": "hsl(325, 100%, 50%)",
					"max": "hsl(263, 45%, 13%)"
				},
				"secondary": {
					"min": "hsl(254, 39%, 85%)",
					"med": "hsl(177, 77%, 44%)",
					"max": "hsl(195, 90%, 12%)"
				}
			},
			"fonts": {
				"heading": ["'Marvin Visions Big'", "sans-serif"],
				"body": ["sans-serif"]
			},
			"images": {},
			"vars": {
				"ckn-bg-color": "var(--c-primary-max)",
				"ckn-text-color": "var(--c-primary-min)",
				"ckn-link-color": "var(--c-primary-med)",
				"ckn-link-active-color": "var(--c-secondary-med)",
				"heading-tracking": "0.05em"
			}
		},
		"vapor": {
			"label": "Vapor",
			"colors": {
				"grey": {
					"min": "hsl(233, 12%, 13%)",
					"med": "hsl(231, 3%, 46%)",
					"max": "hsl(39, 100%, 96%)"
				},
				"primary": {
					"min": "hsl(261, 100%, 31%)",
					"med": "hsl(295, 70%, 45%)",
					"max": "hsl(331, 95%, 92%)"
				},
				"secondary": {
					"min": "hsl(193, 94%, 20%)",
					"med": "hsl(182, 86%, 30%)",
					"max": "hsl(167, 100%, 86%)"
				}
			},
			"fonts": {
				"heading": ["'VCR OSD Mono'", "monospace"],
				"body": ["monospace"]
			},
			"images": {},
			"vars": {
				"ckn-bg-color": "var(--c-primary-max)",
				"ckn-text-color": "var(--c-primary-min)",
				"ckn-link-color": "var(--c-primary-med)",
				"ckn-link-active-color": "var(--c-secondary-med)"
			}
		},
		"y2k": {
			"label": "Y2K",
			"colors": {
				"grey": {
					"min": "#333333",
					"max": "#efefef",
					"med": "#808080"
				},
				"primary": {
					"min": "navy",
					"med": "red",
					"max": "yellow"
				},
				"secondary": {
					"min": "gray",
					"med": "purple",
					"max": "orange"
				}
			},
			"fonts": {
				"heading": ["'Comic Sans MS'", "serif"],
				"body": ["'Times New Roman'", "serif"]
			},
			"images": {},
			"vars": {
				"ckn-bg-color": "var(--c-grey-max)",
				"ckn-text-color": "var(--c-grey-min)",
				"ckn-link-color": "var(--c-primary-med)",
				"ckn-link-active-color": "var(--c-secondary-med)",
				"corner-radius": "0",
				"border-style": "outset"
			}
		}
	},
	"globals": {
		"base-line-height": 1.5
	},
	"typeScale": {
		"0": "clamp(1.00rem, calc(0.80rem + 0.98vw), 1.50rem)",
		"1": "clamp(1.33rem, calc(1.15rem + 0.91vw), 1.80rem)",
		"2": "clamp(1.78rem, calc(1.63rem + 0.75vw), 2.16rem)",
		"3": "clamp(2.37rem, calc(2.28rem + 0.44vw), 2.59rem)",
		"4": "clamp(3.11rem, calc(3.18rem - 0.09vw), 3.16rem)",
		"5": "clamp(3.73rem, calc(4.39rem - 0.93vw), 4.21rem)"
	},
	"size": {
		"2xs": "clamp(0.50rem, calc(0.40rem + 0.49vw), 0.75rem)",
		"xs": "clamp(0.75rem, calc(0.60rem + 0.73vw), 1.13rem)",
		"s": "clamp(1.00rem, calc(0.80rem + 0.98vw), 1.50rem)",
		"m": "clamp(1.50rem, calc(1.21rem + 1.46vw), 2.25rem)",
		"l": "clamp(2.00rem, calc(1.61rem + 1.95vw), 3.00rem)",
		"xl": "clamp(3.00rem, calc(2.41rem + 2.93vw), 4.50rem)",
		"2xl": "clamp(4.00rem, calc(3.22rem + 3.90vw), 6.00rem)"
	},
	"fontFallback": {
		"serif": [
			"'Constantia'",
			"'Lucida Bright'",
			"'Lucidabright'",
			"'Lucida Serif'",
			"'Lucida'",
			"'DejaVu Serif'",
			"'Bitstream Vera Serif'",
			"'Liberation Serif'",
			"'Georgia'",
			"'serif'",
			"'Apple Color Emoji'",
			"'Segoe UI Emoji'",
			"'Segoe UI Symbol'"
		],
		"sans-serif": [
			"'system-ui'",
			"'-apple-system'",
			"'BlinkMacSystemFont'",
			"'Segoe UI'",
			"'Roboto'",
			"'Oxygen'",
			"'Ubuntu'",
			"'Cantarell'",
			"'Fira Sans'",
			"'Droid Sans'",
			"'Helvetica Neue'",
			"'sans-serif'",
			"'Apple Color Emoji'",
			"'Segoe UI Emoji'",
			"'Segoe UI Symbol'"
		],
		"monospace": ["'Monaco'", "'Consolas'", "'Andale Mono'", "'Courier New'", "'monospace'", "'Apple Color Emoji'", "'Segoe UI Emoji'", "'Segoe UI Symbol'"]
	}
}
